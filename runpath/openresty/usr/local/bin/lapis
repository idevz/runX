#!/bin/sh

exec '/usr/local/openresty/luajit/bin/luajit' -e 'package.path="/tmp/build/usr/local/share/lua/5.1/?.lua;/tmp/build/usr/local/share/lua/5.1/?/init.lua;"..package.path; package.cpath="/tmp/build/usr/local/lib/lua/5.1/?.so;"..package.cpath' -e 'local k,l,_=pcall(require,"luarocks.loader") _=k and l.add_context("kong-lapis","1.6.0.1-1")' '/tmp/build/usr/local/lib/luarocks/rocks/kong-lapis/1.6.0.1-1/bin/lapis' "$@"
